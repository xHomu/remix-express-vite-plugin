{
  "name": "remix-vite-express",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "imports": {
    "#*": "./*"
  },
  "scripts": {
    "build": "run-p build:*",
    "dev": "run-p dev:*",
    "start": "run-p start:*",
    "build:remix": "remix vite:build",
    "dev:remix": "vite",
    "start:remix": "cross-env NODE_ENV=production node ./build/server/index.js",
    "build:next": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev:next": "cross-env NODE_OPTIONS=--no-deprecation next dev -p 4000",
    "start:next": "cross-env NODE_OPTIONS=--no-deprecation next start -p 4000",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "3.0.0-beta.25",
    "@payloadcms/next": "3.0.0-beta.25",
    "@payloadcms/richtext-lexical": "3.0.0-beta.25",
    "@remix-run/node": "^2.9.1",
    "@remix-run/react": "^2.9.1",
    "@remix-run/serve": "^2.9.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "graphql": "^16.8.1",
    "http-proxy-middleware": "^3.0.0",
    "isbot": "^4.1.0",
    "morgan": "^1.10.0",
    "next": "14.3.0-canary.7",
    "npm-run-all": "^4.1.5",
    "payload": "3.0.0-beta.25",
    "react": "^19.0.0-beta-94eed63c49-20240425",
    "react-dom": "^19.0.0-beta-94eed63c49-20240425",
    "remix-create-express-app": "^0.3.5"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.9.1",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "remix-express-dev-server": "^0.2.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-env-only": "^2.2.1",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
